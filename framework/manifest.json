{
  "name": "MonoPHP",
  "version": "2.0.0",
  "description": "Single-file PHP framework with Rails-like features",
  "repository": "https://github.com/wilihandarwo/monophp",
  "homepage": "https://monophp.com",
  "license": "MIT",
  "php_version": ">=8.0",
  "entry_point": "public/index.php",
  "features": [
    "validation",
    "query-builder",
    "flash-messages",
    "rate-limiting",
    "password-reset",
    "remember-me",
    "form-helpers",
    "file-caching",
    "json-api",
    "authorization",
    "file-uploads",
    "structured-logging",
    "database-seeding"
  ]
}
